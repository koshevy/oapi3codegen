{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@codegena/ng-api-service": [
        "dist/ng-api-service"
      ],
      "@codegena/ng-api-service/*": [
        "dist/ng-api-service/*"
      ],
      "@codegena/oapi3ts": [
        "dist/oapi3ts"
      ],
      "@codegena/oapi3ts/*": [
        "dist/oapi3ts/*"
      ],
      "@codegena/oapi3ts-cli": [
        "dist/oapi3ts-cli"
      ],
      "@codegena/oapi3ts-cli/*": [
        "dist/oapi3ts-cli/*"
      ],
      "@codegena/todo-app": [
        "dist/todo-app"
      ],
      "@codegena/todo-app/*": [
        "projects/todo-app/*"
      ]
    }
  }
}